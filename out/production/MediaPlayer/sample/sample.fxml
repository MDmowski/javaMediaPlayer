                        <Insets right="10.0" />
                     </HBox.margin></Button>
                  <Button fx:id="stopButton" mnemonicParsing="false" onAction="#stopButtonAction" prefHeight="30.0" prefWidth="30.0">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin></Button>
                  <Button mnemonicParsing="false" onAction="#slowerButtonAction" text="Speed -25%" />
                  <Button mnemonicParsing="false" onAction="#normalSpeedButtonAction" text="Speed 1x" />
                  <Button mnemonicParsing="false" onAction="#fasterButtonAction" text="Speed +25%" />
                  <Button mnemonicParsing="false" onAction="#exitButtonAction" text="Exit" />
